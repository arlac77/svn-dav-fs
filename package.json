{
  "name": "svn-dav-fs",
  "version": "0.0.0-semantic-release",
  "description": "plain JavaScript svn dav",
  "main": "dist/svn.js",
  "jsnext:main": "src/svn.js",
  "module": "src/svn.js",
  "scripts": {
    "cover": "npm run build &&  node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "test": "npm run build && mocha --recursive tests/*",
    "build": "./node_modules/rollup/bin/rollup --format cjs --output dist/svn.js -- src/svn.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/svn-dav-fs.git"
  },
  "keywords": [
    "DAV",
    "SVN"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/svn-dav-fs/issues"
  },
  "homepage": "https://github.com/arlac77/svn-dav-fs#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-datetime": "^1.4.1",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "mocha": "3.0.2",
    "rollup": "^0.36.0",
    "semantic-release": "6.3.0"
  },
  "dependencies": {
    "sax": "^1.2.1",
    "uri-resolver": "^2.1.0"
  },
  "engines": {
    "node": ">=6.5"
  }
}
