{
  "name": "svn-dav-fs",
  "version": "0.0.0-semantic-release",
  "description": "handler for 'svn+https' url scheme (plain js svn dav fs)",
  "main": "dist/svn.js",
  "module": "src/svn.js",
  "scripts": {
    "cover": "npm run prepublish && istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "mocha tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/svn.js -c -- src/svn.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/svn-dav-fs.git"
  },
  "keywords": [
    "DAV",
    "SVN",
    "url-resolver-fs"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/svn-dav-fs/issues"
  },
  "homepage": "https://github.com/arlac77/svn-dav-fs#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.5",
    "cz-conventional-changelog": "^1.2.0",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.40.0"
  },
  "dependencies": {
    "sax": "^1.2.1",
    "url-resolver-fs": "^1.1.3"
  },
  "engines": {
    "node": ">=6.9.2"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
