{
  "name": "svn-dav-fs",
  "version": "0.0.0",
  "description": "svn dav",
  "main": "lib/svn.js",
  "esnext:main": "src/svn.js",
  "scripts": {
    "cover": "node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "compile": "babel --presets es2015 -d lib src",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-core/register --recursive tests/*",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "webpack --optimize-minimize",
    "watch": "webpack --optimize-minimize --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/svn-dav-fs.git"
  },
  "keywords": [
    "dav",
    "svn"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/svn-dav-fs/issues"
  },
  "homepage": "https://github.com/arlac77/svn-dav-fs#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.0",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "3.5.0",
    "copy-webpack-plugin": "^1.1.1",
    "coveralls": "2.11.6",
    "es6-module-loader": "^0.17.10",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "2.4.5",
    "semantic-release": "^6.2.0",
    "webpack": "^2.0.6-beta"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "isomorphic-fetch": "^2.2.1"
  }
}
